[user]
    name  = Wojciech Szaranski
    email = owner@weird.cat

[core]
    excludesfile = ~/.gitignore

[alias]
    co   = checkout
    ci   = commit
    st   = status
    br   = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
